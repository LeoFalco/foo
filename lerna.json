{
    "packages": [
        "packages/*"
    ],
    "version": "independent",
    "command": {
        "publish": {
            "conventionalCommits": true,
            "registry": "https://npm.pkg.github.com/",
            "message": "chore(release): publish",
            "createRelease": "github",
            "fromPackage": true,
            "yes": true
        }
    }
}