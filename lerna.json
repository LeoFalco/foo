{
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "registry": "https://npm.pkg.github.com/",
      "yes": true
    },
    "command": {
      "version": {
        "message": "chore(release): publish",
        "createRelease": "github",
        "yes": true
      }
    }
  }
}
